<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="800.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.MainMenuController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" VBox.vgrow="ALWAYS">
      <children>
            <Button fx:id="logOutButton" layoutX="464.0" layoutY="673.0" mnemonicParsing="false" onAction="#logout" prefHeight="89.0" prefWidth="273.0" text="Log Out">
               <font>
                  <Font size="46.0" />
               </font>
            </Button>
            <Button fx:id="activeBIllsButton" layoutX="113.0" layoutY="112.0" mnemonicParsing="false" onAction="#showActiveBillsPanel" prefHeight="93.0" prefWidth="351.0" text="Active Bills">
               <font>
                  <Font size="46.0" />
               </font>
            </Button>
            <Button fx:id="newBillButton" layoutX="737.0" layoutY="112.0" mnemonicParsing="false" onAction="#showNewBillPanel" prefHeight="93.0" prefWidth="351.0" text="New Bill">
               <font>
                  <Font size="46.0" />
               </font>
            </Button>
            <Label fx:id="loggedUserLabel" alignment="CENTER" layoutY="35.0" prefHeight="40.0" prefWidth="1200.0" text="Logged User: Lorem" textAlignment="CENTER" textFill="RED">
               <font>
                  <Font size="32.0" />
               </font>
            </Label>
            <Button fx:id="raportsButton" layoutX="113.0" layoutY="520.0" mnemonicParsing="false" onAction="#showRaportsPanel" prefHeight="93.0" prefWidth="351.0" text="Raports">
               <font>
                  <Font size="46.0" />
               </font>
            </Button>
            <Button fx:id="stockRoomButton" layoutX="737.0" layoutY="307.0" mnemonicParsing="false" onAction="#showStockRoomPanel" prefHeight="93.0" prefWidth="351.0" text="Stock Room">
               <font>
                  <Font size="46.0" />
               </font>
            </Button>
            <Button fx:id="scheduleButton" layoutX="113.0" layoutY="307.0" mnemonicParsing="false" onAction="#showSchedulePanel" prefHeight="93.0" prefWidth="351.0" text="Schedule">
               <font>
                  <Font size="46.0" />
               </font>
            </Button>
            <Button fx:id="usersButton" layoutX="737.0" layoutY="520.0" mnemonicParsing="false" onAction="#showUsersPanel" prefHeight="93.0" prefWidth="351.0" text="Users">
               <font>
                  <Font size="46.0" />
               </font>
            </Button>
      </children>
    </AnchorPane>
  </children>
</VBox>
