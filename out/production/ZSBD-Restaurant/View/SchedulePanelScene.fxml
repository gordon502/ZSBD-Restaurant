<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="800.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.SchedulePanelController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" VBox.vgrow="ALWAYS">
      <children>
            <Button fx:id="backButton" layoutX="464.0" layoutY="673.0" mnemonicParsing="false" onAction="#showMainMenu" prefHeight="89.0" prefWidth="273.0" text="Back">
               <font>
                  <Font size="46.0" />
               </font>
            </Button>
            <Button fx:id="showScheduleButton" layoutX="32.0" layoutY="115.0" mnemonicParsing="false" prefHeight="93.0" prefWidth="432.0" text="Show Schedule">
               <font>
                  <Font size="46.0" />
               </font>
            </Button>
            <Label fx:id="loggedUserLabel" alignment="CENTER" layoutX="1.0" layoutY="35.0" prefHeight="40.0" prefWidth="1200.0" text="Logged User: Lorem" textAlignment="CENTER" textFill="RED">
               <font>
                  <Font size="32.0" />
               </font>
            </Label>
            <Button fx:id="workedHoursButton" layoutX="737.0" layoutY="115.0" mnemonicParsing="false" prefHeight="93.0" prefWidth="432.0" text="Worked Hours">
               <font>
                  <Font size="46.0" />
               </font>
            </Button>
            <Button fx:id="addShiftButton" layoutX="385.0" layoutY="325.0" mnemonicParsing="false" prefHeight="93.0" prefWidth="432.0" text="Add Shift">
               <font>
                  <Font size="46.0" />
               </font>
            </Button>
            <Button fx:id="editScheduleButton" layoutX="32.0" layoutY="522.0" mnemonicParsing="false" prefHeight="93.0" prefWidth="432.0" text="Edit Schedule">
               <font>
                  <Font size="46.0" />
               </font>
            </Button>
            <Button fx:id="EditShiftButton" layoutX="737.0" layoutY="522.0" mnemonicParsing="false" prefHeight="93.0" prefWidth="432.0" text="Edit Shift">
               <font>
                  <Font size="46.0" />
               </font>
            </Button>
      </children>
    </AnchorPane>
  </children>
</VBox>
