<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="800.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.NewUserPanelController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" VBox.vgrow="ALWAYS">
      <children>
            <Button fx:id="backButton" layoutX="464.0" layoutY="673.0" mnemonicParsing="false" onAction="#showUsersDataPanel" prefHeight="89.0" prefWidth="273.0" text="Back">
               <font>
                  <Font size="46.0" />
               </font>
            </Button>
            <Label fx:id="loggedUserLabel" alignment="CENTER" layoutX="1.0" layoutY="35.0" prefHeight="40.0" prefWidth="1200.0" text="Logged User: Lorem" textAlignment="CENTER" textFill="RED">
               <font>
                  <Font size="32.0" />
               </font>
            </Label>
            <Button fx:id="registerButton" layoutX="389.0" layoutY="551.0" mnemonicParsing="false" onAction="#registerNewUser" prefHeight="93.0" prefWidth="432.0" text="Register">
               <font>
                  <Font size="46.0" />
               </font>
            </Button>
            <TextField fx:id="loginTextField" alignment="CENTER" layoutX="207.0" layoutY="208.0" prefHeight="65.0" prefWidth="332.0" promptText="login">
               <font>
                  <Font size="33.0" />
               </font>
            </TextField>
            <Label alignment="CENTER" layoutX="1.0" layoutY="102.0" prefHeight="40.0" prefWidth="1200.0" text="Fill fields with necessary data." textAlignment="CENTER">
               <font>
                  <Font size="32.0" />
               </font>
            </Label>
            <Label alignment="CENTER" layoutX="-1.0" layoutY="220.0" prefHeight="40.0" prefWidth="184.0" text="login:" textAlignment="CENTER">
               <font>
                  <Font size="32.0" />
               </font>
            </Label>
            <Label alignment="CENTER" layoutX="-25.0" layoutY="347.0" prefHeight="40.0" prefWidth="232.0" text="password:" textAlignment="CENTER">
               <font>
                  <Font size="32.0" />
               </font>
            </Label>
            <PasswordField fx:id="passwordTextField" alignment="CENTER" layoutX="207.0" layoutY="334.0" prefHeight="65.0" prefWidth="332.0" promptText="password">
               <font>
                  <Font size="33.0" />
               </font>
            </PasswordField>
            <Label alignment="CENTER" layoutX="-75.0" layoutY="466.0" prefHeight="40.0" prefWidth="332.0" text="function: " textAlignment="CENTER">
               <font>
                  <Font size="32.0" />
               </font>
            </Label>
            <ComboBox fx:id="functionsComboBox" layoutX="255.0" layoutY="454.0" prefHeight="65.0" prefWidth="257.0" promptText="function" />
            <Label alignment="CENTER" layoutX="613.0" layoutY="221.0" prefHeight="40.0" prefWidth="197.0" text="first name: " textAlignment="CENTER">
               <font>
                  <Font size="32.0" />
               </font>
            </Label>
            <Label alignment="CENTER" layoutX="613.0" layoutY="347.0" prefHeight="40.0" prefWidth="197.0" text="surname: " textAlignment="CENTER">
               <font>
                  <Font size="32.0" />
               </font>
            </Label>
            <TextField fx:id="firstNameTextField" alignment="CENTER" layoutX="838.0" layoutY="208.0" prefHeight="65.0" prefWidth="332.0" promptText="first name">
               <font>
                  <Font size="33.0" />
               </font>
            </TextField>
            <TextField fx:id="surnameTextField" alignment="CENTER" layoutX="838.0" layoutY="335.0" prefHeight="65.0" prefWidth="332.0" promptText="surname">
               <font>
                  <Font size="33.0" />
               </font>
            </TextField>
            <Label alignment="CENTER" layoutX="613.0" layoutY="467.0" prefHeight="40.0" prefWidth="197.0" text="phone: " textAlignment="CENTER">
               <font>
                  <Font size="32.0" />
               </font>
            </Label>
            <TextField fx:id="phoneTextField" alignment="CENTER" layoutX="838.0" layoutY="454.0" prefHeight="65.0" prefWidth="332.0" promptText="phone">
               <font>
                  <Font size="33.0" />
               </font>
            </TextField>
      </children>
    </AnchorPane>
  </children>
</VBox>
