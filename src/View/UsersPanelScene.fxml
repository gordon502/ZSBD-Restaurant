<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="TOP_CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.UsersPanelController">
  <children>
      <Pane prefHeight="38.0" />
      <Label fx:id="loggedUserLabel" alignment="CENTER" text="Logged User: Lorem" textAlignment="CENTER" textFill="RED">
         <font>
            <Font size="32.0" />
         </font>
      </Label>
      <Pane prefHeight="22.0" />
      <Button fx:id="addNewUserButton" mnemonicParsing="false" onAction="#showNewUserPanel" prefWidth="350.0" text="Add New User">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
      <Button fx:id="modifyDataButton" mnemonicParsing="false" prefWidth="350.0" text="Modify Data">
         <font>
            <Font size="20.0" />
         </font>
      </Button>
      <Pane prefHeight="52.0" prefWidth="350.0" />
      <Button fx:id="backButton" mnemonicParsing="false" onAction="#showMainMenu" prefHeight="89.0" prefWidth="273.0" text="Back">
         <font>
            <Font size="20.0" />
         </font>
         <VBox.margin>
            <Insets bottom="40.0" top="20.0" />
         </VBox.margin>
      </Button>
  </children>
</VBox>
