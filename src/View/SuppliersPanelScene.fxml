<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<BorderPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.SuppliersPanelController">
   <left>
      <VBox alignment="CENTER" maxWidth="300.0" minWidth="300.0" prefHeight="600.0" prefWidth="300.0" spacing="5.0" BorderPane.alignment="CENTER">
         <children>
            <GridPane fx:id="dataGrid" alignment="TOP_CENTER" hgap="10.0" nodeOrientation="LEFT_TO_RIGHT" prefHeight="214.0" prefWidth="260.0" vgap="10.0">
              <columnConstraints>
                <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" maxWidth="203.3333282470703" minWidth="10.0" prefWidth="158.0" />
                <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" maxWidth="262.0" minWidth="10.0" prefWidth="262.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints maxHeight="72.0" minHeight="10.0" prefHeight="72.0" vgrow="SOMETIMES" />
                <RowConstraints maxHeight="102.0" minHeight="10.0" prefHeight="59.0" vgrow="SOMETIMES" />
                <RowConstraints maxHeight="63.0" minHeight="10.0" prefHeight="63.0" vgrow="SOMETIMES" />
                  <RowConstraints maxHeight="63.0" minHeight="10.0" prefHeight="63.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Name" GridPane.halignment="CENTER" />
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Price" GridPane.halignment="CENTER" GridPane.rowIndex="1" />
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Food Category" GridPane.halignment="CENTER" GridPane.rowIndex="2" />
                  <TextField fx:id="phoneTextField" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                  <TextField fx:id="nameTextField" GridPane.columnIndex="1" />
                  <TextField fx:id="vatTextField" prefWidth="177.0" GridPane.columnIndex="1" GridPane.rowIndex="3" />
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="VAT" GridPane.rowIndex="3" />
                  <ComboBox fx:id="foodCategoryComboBox" prefWidth="150.0" GridPane.columnIndex="1" GridPane.rowIndex="2" />
               </children>
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" right="30.0" top="10.0" />
               </VBox.margin>
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
            </GridPane>
            <Label fx:id="userLabel" />
            <Button fx:id="addButton" mnemonicParsing="false" onAction="#addSupplier" prefHeight="25.0" prefWidth="98.0" text="Add">
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </Button>
            <Button fx:id="modifyButton" mnemonicParsing="false" onAction="#modifySupplier" prefHeight="25.0" prefWidth="98.0" text="Modify" />
            <Button fx:id="backButton" mnemonicParsing="false" onAction="#showStockRoomPanel" prefHeight="25.0" prefWidth="98.0" text="Back">
               <VBox.margin>
                  <Insets top="20.0" />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
   </left>
   <center>
      <TableView fx:id="supplierTable" prefHeight="600.0" prefWidth="0.0" BorderPane.alignment="CENTER">
        <columns>
            <TableColumn minWidth="0.0" prefWidth="100.0" text="FoodId" />
            <TableColumn prefWidth="100.0" text="Name" />
            <TableColumn prefWidth="100.0" text="Price" />
            <TableColumn prefWidth="100.0" text="FoodCategory" />
            <TableColumn prefWidth="100.0" text="VAT" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
         <BorderPane.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </BorderPane.margin>
      </TableView>
   </center>
</BorderPane>
